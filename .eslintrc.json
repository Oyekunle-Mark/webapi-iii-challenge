{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": ["airbnb-base"],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "rules": {
    "arrow-parens": 0,
    "curly": 0,
    "nonblock-statement-body-position": 0,
    "no-shadow": 0,
    "no-restricted-globals": 0,
    "consistent-return": 0
  }
}
